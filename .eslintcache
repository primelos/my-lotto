\n\n  function sortNums(a, b) {\n    return a - b;\n  }\n\n  function lottery() {\n    let fiveNums = [];\n    let megaNum = [];\n\n    while (fiveNums.length < 5) {\n      let emptyArray = new Uint32Array(1);\n      let ranNum = window.crypto.getRandomValues(emptyArray);\n      let mixedNum = Math.floor((ranNum * 5) / 100000000);\n      if (mixedNum > 0 && mixedNum <= 47 && !fiveNums.includes(mixedNum)) {\n        fiveNums.push(mixedNum);\n      }\n    }\n    while (megaNum.length < 1) {\n      let emptyArray = new Uint32Array(1);\n      let ranNum = window.crypto.getRandomValues(emptyArray);\n      let numToPush = Math.floor(ranNum[0] / 100000000);\n      if (numToPush > 0 && numToPush <= 27) {\n        megaNum.push(numToPush);\n      }\n    }\n    return { fiveNums, megaNum };\n  }\n\n  function handleNumbers() {\n    let pickNumbers = lottery();\n    setNum((prev) => (prev = pickNumbers.fiveNums.sort(sortNums)));\n    setMega((prev) => (prev = pickNumbers.megaNum));\n    setGameNumbers((x) => [...x, [pickNumbers.fiveNums, pickNumbers.megaNum]]);\n  }\n  console.log(\"here\", gameNumbers);\n  // useEffect(() => {\n  //   console.log('in useEffect',num)\n\n  // }, [num])\n\n  return (\n    <div className=\"lotto-container\">\n      <div className=\"goback\" onClick={handleClick}>\n        <button onClick={handleClick}>Home</button>\n      </div>\n\n      <p className=\"title\">Super Lotto</p>\n      <div className=\"game-results\">\n        <div className=\"five-numbers\">\n          {num.length === 0 ? \"\" : <p>Numbers to play</p>}\n\n          <div className=\"number-container\">\n            {num.map((n, i) => (\n              <div className=\"box-num\">\n                <p className=\"win-num\">{n}</p>\n              </div>\n            ))}\n          </div>\n        </div>\n\n        <div className=\"mega\">\n          {num.length === 0 ? \"\" : <p>Mega</p>}\n\n          {mega.length === 0 ? (\n            \"\"\n          ) : (\n            <div className=\"box-num\">\n              <p className=\"win-num\">{mega}</p>\n            </div>\n          )}\n        </div>\n      </div>\n      <div className=\"box-button\">\n        {num.length < 1 ? (\n          <button onClick={handleNumbers}>PLAY</button>\n        ) : (\n          <button onClick={handleNumbers}>GO AGAIN</button>\n        )}\n      </div>\n\n      <GameHistory\n        show={show}\n        gameNumbers={gameNumbers}\n        showResults={showResults}\n      />\n      <GameDays\n        day1=\"Wednesday\"\n        day2=\"Saturday\"\n        time=\"7:45pm\"\n        url=\"https://www.calottery.com/draw-games/superlotto-plus\"\n      />\n    </div>\n  );\n};\n\nexport default Lotto;\n","/Users/alearanibar/codingcfv/reactpractice/lottery/src/components/mega/index.jsx",[],"/Users/alearanibar/codingcfv/reactpractice/lottery/src/components/power/index.jsx",["49"],"/Users/alearanibar/codingcfv/reactpractice/lottery/src/pages/homepage/index.jsx",[],"/Users/alearanibar/codingcfv/reactpractice/lottery/src/components/nav/index.jsx",[],"/Users/alearanibar/codingcfv/reactpractice/lottery/src/components/game-days/index.jsx",["50"],"/Users/alearanibar/codingcfv/reactpractice/lottery/src/components/game-history/index.jsx",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":3,"column":8,"nodeType":"57","messageId":"58","endLine":3,"endColumn":14},{"ruleId":"55","severity":1,"message":"59","line":4,"column":8,"nodeType":"57","messageId":"58","endLine":4,"endColumn":12},{"ruleId":"55","severity":1,"message":"60","line":5,"column":8,"nodeType":"57","messageId":"58","endLine":5,"endColumn":16},{"ruleId":"55","severity":1,"message":"61","line":10,"column":23,"nodeType":"57","messageId":"58","endLine":10,"endColumn":37},{"ruleId":"62","severity":1,"message":"63","line":16,"column":7,"nodeType":"64","endLine":16,"endColumn":37},"no-native-reassign",["65"],"no-negated-in-lhs",["66"],"no-unused-vars","'Button' is d